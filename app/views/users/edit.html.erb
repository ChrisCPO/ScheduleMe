<h3> Edit My Info </h3>

<%= form_for @user, method:"put"  do |form| %>

  <%= render "errors" %>

  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div>
    <%= form.submit "Submit New Info" %>
  </div>
<% end %>
